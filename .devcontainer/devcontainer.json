{
  "name": "Lisp Development for Mac (arm64)",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "workspaceMount": "source=${localWorkspaceFolder},target=/root/.roswell/lisp/quicklisp/local-projects/${localWorkspaceFolderBasename},type=bind",
  "workspaceFolder": "/root/.roswell/lisp/quicklisp/local-projects/${localWorkspaceFolderBasename}",
  "customizations": {
    "vscode": {
      "extensions": [
        "qingpeng.common-lisp",
        "streetsidesoftware.code-spell-checker"
      ]
    }
  }
}
